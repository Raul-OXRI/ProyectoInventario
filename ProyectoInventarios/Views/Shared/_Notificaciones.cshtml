@using ProyectoInventarios_Utilidades

@if (TempData[DefinicionesEstaticas.Exitosa] != null)
{
    @*usaremos las libreria de jqayu y la de javas *@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js"></script>
    <script type="text/javascript">
        toastr.success('@TempData[DefinicionesEstaticas.Exitosa]');
    </script>
}

@if (TempData[DefinicionesEstaticas.Error] != null)
{
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/2.1.4/toastr.min.js"></script>
    <script type="text/javascript">
        toastr.error('@TempData[DefinicionesEstaticas.Error]');
    </script>
}